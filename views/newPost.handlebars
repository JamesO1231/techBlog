<section id='newPost'>
    <h3>Make a New Post</h3>
    <div id='holder'>
        <div class="row px-3 form-item"><label class="mb-1">
                <h6 class="mb-0 text-sm">Title</h6>
            </label><input name="title" placeholder="Enter Post Title" id='titleEntry'></div>
        <div class="row px-3 form-item"><label class="mb-1">
                <h6 class="mb-0 text-sm">Content</h6></label>
            <textarea class="form-control" id='contentEntry' rows="10"></textarea>
        </div>
        <div class="row px-3 form-item"><label class="mb-1">
                <h6 class="mb-0 text-sm">Username</h6>
            </label><input name="username" placeholder="Enter your username" id='usernameLog'></div>
        <div class="row px-3 mb-4">
        </div>
        <div class="row mb-3 px-3"><button type="button" class="btn btn-primary" id='newForm'>Submit</button></div>
    </div>
</section>
<script src="/public/js/newPost.js"></script>